{
  "name": "Shared bookmark list",
  "version": "1.0",
  "manifest_version": 3,
  "action": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./static/js/content.js"]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "./static/js/background.js",
    "type": "module"
  },
  "permissions": ["tabs", "activeTab", "storage", "bookmarks"],
  "icons": {
    "16": "logox1.png",
    "48": "logox2.png",
    "128": "logox2.png"
  }
}
